{
  "custom_commands": {
    "test": {
      "description": "Test ClaudeBox custom command",
      "command": "node ~/.claude-custom/commands/test-command.js"
    },
    "project-info": {
      "description": "Display project information",
      "command": "node ~/.claude-custom/commands/project-info.js"
    },
    "git-status": {
      "description": "Enhanced git status",
      "command": "bash ~/.claude-custom/hooks/git-helper.sh"
    },
    "feature-sync": {
      "description": "Append concise progress/context to a feature notes markdown file",
      "command": "node ~/.claude-custom/commands/feature-sync.js"
    }
  },
  "aliases": {
    "cb": "custom_commands",
    "info": "project-info",
    "gs": "git-status",
    "feat": "feature-sync",
    "fs": "feature-sync",
    "cbfeat": "feature-sync"
  },
  "environment": {
    "CLAUDEBOX_CUSTOM": "enabled",
    "CLAUDEBOX_VERSION": "custom-build"
  }
}